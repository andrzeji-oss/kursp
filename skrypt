name = input("Podaj imię: ")
lastName = input("Podaj nazwisko: ")
birthDate = input("Podaj datę urodzenia (YYYY-MM-DD): ")
possition = input("Podaj stanowisko: ")
salary = float(input("Wprowadź wynagrodzenie netto: "))

print(name, lastName, birthDate, possition, str(salary) + " zl", str(salary * 1.23) + " zl", sep=" | ")

print(type(name))

salary = 1000.
isActivated = True
element = 1
print(type(name), type(salary), type(isActivated), type(element))